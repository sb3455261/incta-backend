{
    "name": "incta-backend",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build:gateway": "sh set-env-prod.sh sh -c 'nest build gateway'",
        "start:gateway": "sh set-env-prod.sh sh -c 'nest start gateway'",
        "test:e2e:gateway": "jest --config ./apps/gateway/test/jest-e2e.json",
        "build:auth": "sh set-env-prod.sh sh -c 'nest build auth'",
        "start:auth": "sh set-env-prod.sh sh -c 'nest start auth'",
        "test:e2e:auth": "jest --config ./apps/auth/test/jest-e2e.json",
        "prisma:generate:users": "cd apps/users && prisma generate",
        "prisma:migrate:users": "prisma migrate deploy --schema=./apps/users/prisma/schema.prisma",
        "build:users": "sh set-env-prod.sh sh -c 'npm run prisma:generate:users && rm -rf dist/apps/users/prisma && mkdir -p dist/apps/users/prisma && cp -r apps/users/prisma/generated dist/apps/users/prisma/generated && npm run prisma:migrate:users && nest build users'",
        "start:users": "sh set-env-prod.sh sh -c 'nest start users'",
        "test:e2e:users": "jest --config ./apps/users/test/jest-e2e.json",
        "build": "sh -c 'npm run build:gateway & npm run build:users  & npm run build:auth'",
        "start": "sh -c 'npm run start:gateway & npm run start:users & npm run start:auth'",
        ":format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
        ":lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "format": "npm run :format && npm run :lint",
        "test:e2e": "sh set-env-dev.sh sh -c 'npm run test:e2e:auth && npm run test:e2e:gateway && npm run prisma:generate:users && npm run test:e2e:users'",
        "test": "sh set-env-dev.sh sh -c 'jest --detectOpenHandles'",
        "dev:gateway": "sh set-env-dev.sh sh -c 'ts-node-dev -r tsconfig-paths/register --respawn apps/gateway/src/main.ts'",
        "dev:auth": "sh set-env-dev.sh sh -c 'ts-node-dev -r tsconfig-paths/register --respawn apps/auth/src/main.ts'",
        "dev:users": "sh set-env-dev.sh sh -c 'npm run prisma:generate:users && ts-node-dev -r tsconfig-paths/register --respawn apps/users/src/main.ts'",
        "dev": "sh set-env-dev.sh sh -c 'concurrently -n \"GWY,AUT,USR\" -c \"blue,red,green\" \"npm run dev:gateway\" \"npm run dev:auth\" \"npm run dev:users\"'"
    },
    "dependencies": {
        "@nestjs-modules/mailer": "^1.11.2",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.2.3",
        "@nestjs/core": "^10.0.0",
        "@nestjs/cqrs": "^10.2.7",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/microservices": "^10.3.10",
        "@nestjs/mongoose": "^10.0.10",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/schedule": "^4.1.0",
        "@nestjs/swagger": "^7.4.0",
        "@prisma/client": "^5.17.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.4.5",
        "joi": "^17.13.3",
        "mongoose": "^8.5.2",
        "passport": "^0.7.0",
        "passport-github2": "^0.1.12",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/passport-jwt": "^4.0.1",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "concurrently": "^8.2.2",
        "eslint": "^8.42.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "prisma": "^5.17.0",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/apps/",
            "<rootDir>/libs/"
        ],
        "moduleNameMapper": {
            "^@app/shared(|/.*)$": "<rootDir>/libs/shared/src/$1"
        }
    }
}
